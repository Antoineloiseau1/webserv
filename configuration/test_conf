server
		server_name			example.com
		listen					8079
		cgi_extension			.cgi
		autoindex	off;
		client_max_body_size	100
		error_page   404 413 501 505 data/www/error/error_test.html;
		location data/www/
		{
			limit_except	GET
			cgi_extension			.php
			autoindex on
		}

