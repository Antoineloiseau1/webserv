server
		server_name			example.com
		listen					4242
		cgi_extension			.cgi
		autoindex				off;
		index					milan.html
		error_page		 501		data/www/error/error_test.html
		client_max_body_size	10000000
		location data/www/
		{
			limit_except		GET POST
			cgi_extension			.php
			autoindex off
			index				index.html milan.html
		}
		location milan
		{
			root data/www/
		}

