server
		listen					4242
		cgi_extension			.cgi
		autoindex				off;
		index					data/www/milan.html
		error_page		 501		data/www/error/error_test.html
		location data/www/
		{
			limit_except		GET POST
			cgi_extension			.php
			autoindex on
			index				index.html data/www/milan.html
		}
		location milan
		{
			root data/www/
		}

server
